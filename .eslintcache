ist.Item as='a' onClick={() => { history.push('/terms-and-conditions') }}>Terms & Conditions</List.Item>\n              </List>\n            </Grid.Column>\n            <Grid.Column width={3}>\n              <Header inverted as='h4' content='Games' />\n              <List link inverted>\n                <List.Item as='a' onClick={() => { history.push('/milkshake-jump') }}>Milkshake Jump</List.Item>\n              </List>\n            </Grid.Column>\n            <Grid.Column width={7}>\n              <Header as='h4' inverted>\n                Social Media\n              </Header>\n\n\n              <List horizontal>\n                <List.Item as=\"a\" target=\"_blank\" href='https://www.facebook.com'>\n                <Icon link={true} color=\"blue\" name=\"facebook\" size=\"big\" />\n                </List.Item>\n                <List.Item as=\"a\" target=\"_blank\" href='https://www.twitter.com'>\n                <Icon link={true} color=\"blue\" name=\"twitter\" size=\"big\" />\n                </List.Item>\n                <List.Item as=\"a\" target=\"_blank\" href='https://www.youtube.com'>\n                <Icon link={true} color=\"red\" name=\"youtube\" size=\"big\" />\n                </List.Item>\n                <List.Item as=\"a\" target=\"_blank\" href=\"mailto:thien@battlekoi.com?subject='Battle Koi Website Enquiry'\">\n                <Icon link={true} color=\"grey\" name=\"mail\" size=\"big\" />\n                </List.Item>\n              </List>\n            </Grid.Column>\n            \n          </Grid.Row>\n          <Grid.Row>\n            {copyright}\n          </Grid.Row>\n        </Grid>\n      </Container>\n    </Segment>\n  )\n}\n\nexport default Footer;","/home/thien/programming/Battle Koi/battle-koi/src/components/desktop-layout.js",[],"/home/thien/programming/Battle Koi/battle-koi/src/components/privacy.js",["99","100","101","102"],"import React from 'react';\nimport { Container, Grid, Header, Placeholder, Segment } from 'semantic-ui-reac